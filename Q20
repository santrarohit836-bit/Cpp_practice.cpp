// 20. Program to check if a number is positive/negative/zero.

#include <iostream>
using namespace std;

int main() {
    int num = -5;

    if (num > 0)
        cout << "Positive" << endl;
    else if (num < 0)
        cout << "Negative" << endl;
    else
        cout << "Zero" << endl;

    return 0;
}

/*
Output:
Negative
*/

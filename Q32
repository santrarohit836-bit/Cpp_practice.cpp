// 32. Program to find largest element in array.

#include <iostream>
using namespace std;

int main() {
    int arr[5] = {10, 45, 23, 89, 12};
    int max = arr[0];

    for (int i = 1; i < 5; i++) {
        if (arr[i] > max)
            max = arr[i];
    }

    cout << max;
    return 0;
}

/*
Output:
89
*/

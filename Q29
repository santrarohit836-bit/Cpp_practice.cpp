// 29. Program demonstrating break and continue.

#include <iostream>
using namespace std;

int main() {
    for (int i = 1; i <= 10; i++) {

        if (i == 3)
            continue;   // skips 3

        if (i == 7)
            break;      // stops loop at 7

        cout << i << " ";
    }
    return 0;
}

/*
Output:
1 2 4 5 6
*/

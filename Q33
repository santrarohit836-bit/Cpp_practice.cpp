// 33. Program to find second-largest element.

#include <iostream>
using namespace std;

int main() {
    int arr[5] = {10, 45, 23, 89, 12};
    int first = arr[0], second = arr[0];

    for (int i = 0; i < 5; i++) {
        if (arr[i] > first) {
            second = first;
            first = arr[i];
        } else if (arr[i] > second && arr[i] != first) {
            second = arr[i];
        }
    }

    cout << second;
    return 0;
}

/*
Output:
45
*/

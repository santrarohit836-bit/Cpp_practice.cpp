// 38. Program to copy string manually.

#include <iostream>
using namespace std;

int main() {
    char str1[] = "Cplusplus";
    char str2[20];
    int i = 0;

    while (str1[i] != '\0') {
        str2[i] = str1[i];
        i++;
    }
    str2[i] = '\0';

    cout << str2;
    return 0;
}

/*
Output:
Cplusplus
*/
